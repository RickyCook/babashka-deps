[user]
	name = Ricky Cook
	email = mail@thatpanda.com
[core]
	editor = /usr/bin/vim
	quotepath = false
	pager = diff-so-fancy | less --tabs=4 -RFX
[push]
	default = matching
[help]
	autocorrect = 3
[credential]
	helper = osxkeychain
[filter "lfs"]
	required = true
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
